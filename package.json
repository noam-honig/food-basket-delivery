{
  "name": "hug-moms",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "gc": "ng generate component --spec false",
    "ng-serve": "ng serve",
    "ng-serve-noam-ip-10.0.0.6": "ng serve --host 10.0.0.6",
    "start": "npm run server:execute",
    "build": "npm run server:build && npm run ng-build",
    "ng-build": "ng build --prod",
    "server:build": "tsc -p tsconfig.server.json",
    "server:build-watch": "tsc -p tsconfig.server.json --watch",
    "publish-to-heroku-test": "git push heroku master -f",
    "publish-to-salmaz": "git push salmaz master -f",
    "publish-to-heroku-test-old": "git push hug-moms-old latest-release:master -f",
    "copy-db": "heroku pg:copy hugmoms::DATABASE_URL hugmoms2::DATABASE_URL -a hugmoms2",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server:execute": "node dist-server/app/server/server.js",
    "build:stats": "ng build  --stats-json",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
